!!! 5
html(lang='en')
  head
    title= title+' | Opinionwall, opinions and surveys'
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
    meta(name='viewport', content='width=device-width,initial-scale=1')  
  
    meta(http-equiv='Content-type', content='text/html;charset=UTF-8')
    meta(name="keywords", content="surveys, trusted, opinions, vote, poll, statistics, trends")
    meta(name="description", content="Opinionwall is a free tool to interact with your community, opinions, surveys, polls and statistics, everything you need to find out what are the trends and people choices.")
  
    link(href='/favicon.ico', rel='shortcut icon', type='image/x-icon')
    
    link(rel='stylesheet', href='/css/app.css')
  body
    include header
    block content
                                  
    script(type='text/javascript', src='https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js')
    script(type='text/javascript', src='//netdna.bootstrapcdn.com/twitter-bootstrap/2.2.1/js/bootstrap.min.js')
    script(type='text/javascript', src='/js/app.js')  
    
    block custom_page_script